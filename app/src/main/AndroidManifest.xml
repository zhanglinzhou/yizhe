<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dashen.xiangmu">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".avtivity.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".avtivity.AdActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".avtivity.WelcomeActivity" />
        <activity android:name=".avtivity.MainActivity" />
        <activity android:name=".avtivity.TeMaiShopActivity" />
        <activity android:name=".avtivity.TeMaiGoodsActivity" />
        <activity android:name=".avtivity.SearchActivity" />
        <activity android:name=".adapter.VideoActivity"
            android:screenOrientation="landscape"></activity>
    </application>

</manifest>